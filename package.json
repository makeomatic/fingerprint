{
  "name": "fprint",
  "version": "1.0.0",
  "description": "returns hash digest (fingerprint) of a given buffer, file by it's absolute or relative path or stream in node.js",
  "main": "./dist/index.js",
  "scripts": {
    "check-types": "tsc --noEmit",
    "prepack": "tsc --outDir dist --sourceMap --declaration",
    "test": "mocha --require=ts-node/register -R spec test/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makeomatic/fingerprint.git"
  },
  "keywords": [
    "hash",
    "digest",
    "fingerprint",
    "static",
    "asset",
    "md5",
    "sha"
  ],
  "author": "Vitaly Aminev <v@makeomatic.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/fingerprint/issues"
  },
  "homepage": "https://github.com/makeomatic/fingerprint#readme",
  "dependencies": {
    "callsite": "^1.0.0",
    "isstream": "^0.1.2"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/glob": "7.1.1",
    "@types/mocha": "7.0.2",
    "@types/callsite": "1.0.30",
    "@types/isstream": "0.1.0",
    "@types/node": "13.13.1",
    "chai": "^4.2.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "ts-node": "8.9.0",
    "xo": "0.29.1",
    "typescript": "3.8.3"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "rules": {
      "@typescript-eslint/promise-function-async": "off",
      "@typescript-eslint/prefer-readonly-parameter-types": "off",
      "@typescript-eslint/no-unsafe-call": "off"
    }
  }
}
